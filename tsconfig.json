{
  "compilerOptions": {
    "lib": [
      "es2016",
      "es2017.object",
      "dom"
    ],
    "allowJs": false,
    "target": "es2016",
    "outDir": "dist",
    "module": "commonjs",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "esModuleInterop": true
  }
}